{
  "name": "2025-HF-Tinder",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/Tinder/backend",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/Tinder/backend"
      },
      "configurations": {
        "development": {
          "outputPath": "dist/apps/Tinder/backend"
        }
      },
      "dependsOn": [
        {
          "target": "codegen",
          "params": "forward"
        }
      ]
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "2025-HF-Tinder:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "2025-HF-Tinder:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "2025-HF-Tinder:build:production",
          "dev": false
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/Tinder/backend/jest.config.ts",
        "passWithNoTests": true,
        "codeCoverage": true
      },
      "dependsOn": [
        {
          "target": "codegen",
          "params": "forward"
        }
      ]
    },
    "get-secrets:dev": {
      "executor": "@pinecone-intern-monorepo/secrets:get",
      "defaultConfiguration": "development",
      "configurations": {
        "development": {
          "groups": ["Tinder-2hf-2024-BE"],
          "env": "dev"
        }
      }
    },
    "get-secrets:testing": {
      "executor": "@pinecone-intern-monorepo/secrets:get",
      "defaultConfiguration": "testing",
      "configurations": {
        "testing": {
          "groups": ["Tinder-2hf-2024-BE"],
          "env": "test"
        }
      }
    },
    "get-secrets:prod": {
      "executor": "@pinecone-intern-monorepo/secrets:get",
      "defaultConfiguration": "prod",
      "configurations": {
        "prod": {
          "groups": ["Tinder-2hf-2024-BE"],
          "env": "prod"
        }
      }
    },
    "codegen": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["graphql-codegen --config ./apps/Tinder/backend/codegen.ts"]
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "2025-HF-Tinder:build:production"
      }
    },
    "export-env": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["export $(cat apps/Tinder/backend/.env | xargs)"],
        "parallel": false
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/Tinder/backend/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["rm -rf dist/apps/Tinder/backend", "rm -rf .vercel"]
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx dotenv -v VERCEL_ORG_ID=team_qW7CWHt1mKoH5ti6SFJsQhC7 -v VERCEL_PROJECT_ID=prj_8iTnJSCDZt3xeFQn11A84PUK4hfT -- vercel pull --yes --environment=preview --token=$VERCEL_TOKEN",
          "npx env-cmd -f apps/Tinder/backend/.env vercel build --token=$VERCEL_TOKEN",
          "npx dotenv -v VERCEL_ORG_ID=team_qW7CWHt1mKoH5ti6SFJsQhC7 -v VERCEL_PROJECT_ID=prj_8iTnJSCDZt3xeFQn11A84PUK4hfT -- vercel --archive=tgz --prebuilt --token=$VERCEL_TOKEN"
        ],
        "parallel": false
      },
      "dependsOn": [
        {
          "target": "clean"
        },
        {
          "target": "get-secrets:testing --verbose"
        },
        {
          "target": "export-env"
        },
        {
          "target": "codegen",
          "params": "forward"
        }
      ]
    },
    "deploy-dev": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx dotenv -v VERCEL_ORG_ID=team_qW7CWHt1mKoH5ti6SFJsQhC7 -v VERCEL_PROJECT_ID=prj_ADcsZIEIH0QwREWbtZ9gZzLtf7IA -- vercel pull --yes --environment=production --token=$VERCEL_TOKEN",
          "vercel build --prod --token=$VERCEL_TOKEN",
          "npx dotenv -v VERCEL_ORG_ID=team_qW7CWHt1mKoH5ti6SFJsQhC7 -v VERCEL_PROJECT_ID=prj_ADcsZIEIH0QwREWbtZ9gZzLtf7IA -- vercel --prebuilt --prod --token=$VERCEL_TOKEN"
        ],
        "parallel": false
      },
      "dependsOn": [
        {
          "target": "clean"
        },
        {
          "target": "get-secrets:dev --verbose"
        },
        {
          "target": "export-env"
        },
        {
          "target": "codegen",
          "params": "forward"
        }
      ]
    },
    "deploy-prod": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx dotenv -v VERCEL_ORG_ID=team_qW7CWHt1mKoH5ti6SFJsQhC7 -v VERCEL_PROJECT_ID=prj_R0IVgc22jzlyoooZgGH1VJSvfAXq -- vercel pull --yes --environment=production --token=$VERCEL_TOKEN",
          "vercel build --prod --token=$VERCEL_TOKEN",
          "npx dotenv -v VERCEL_ORG_ID=team_qW7CWHt1mKoH5ti6SFJsQhC7 -v VERCEL_PROJECT_ID=prj_R0IVgc22jzlyoooZgGH1VJSvfAXq -- vercel --prebuilt --prod --token=$VERCEL_TOKEN"
        ],
        "parallel": false
      },
      "dependsOn": [
        {
          "target": "clean"
        },
        {
          "target": "get-secrets:prod --verbose"
        },
        {
          "target": "export-env"
        },
        {
          "target": "codegen",
          "params": "forward"
        }
      ]
    }
  },
  "tags": []
}
