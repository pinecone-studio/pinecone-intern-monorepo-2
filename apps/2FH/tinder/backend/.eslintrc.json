{
  "extends": [
    "plugin:@nx/react-typescript",
    "next",
    "next/core-web-vitals",
    "../../../../.eslintrc.json"
  ],
  "ignorePatterns": [
    "node_modules",
    "dist",
    "coverage",
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/__tests__/**",
    "specs/**"
  ],
  "overrides": [
    {
      "files": [
        "*.*"
      ],
      "rules": {
        "@next/next/no-html-link-for-pages": "off",
        "complexity": "off",
        "@typescript-eslint/no-explicit-any": "off"
      }
    },
    {
      "files": [
        "*.ts",
        "*.tsx",
        "*.js",
        "*.jsx"
      ],
      "rules": {
        "@next/next/no-html-link-for-pages": "off"
      }
    },
    {
      "files": [
        "*.ts",
        "*.tsx"
      ],
      "rules": {
        "complexity": "off",
        "@typescript-eslint/no-explicit-any": "off",
        "@next/next/no-html-link-for-pages": "off"
      }
    },
    {
      "files": [
        "*.js",
        "*.jsx"
      ],
      "rules": {}
    },
    {
      "files": [
        "*.spec.ts",
        "*.spec.tsx",
        "*.spec.js",
        "*.spec.jsx"
      ],
      "env": {
        "jest": true
      }
    }
  ]
}