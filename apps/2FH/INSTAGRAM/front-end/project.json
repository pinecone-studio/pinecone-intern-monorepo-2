{
  "name": "front-end",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/2FH/INSTAGRAM/front-end",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/apps/2FH/INSTAGRAM/front-end"
      },
      "configurations": {
        "development": {
          "outputPath": "dist/apps/2FH/INSTAGRAM/front-end"
        }
      },
      "dependsOn": [
        {
          "target": "codegen:dev",
          "params": "forward"
        }
      ]
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "front-end:build",
        "dev": true
      },
      "configurations": {
        "development": {
          "buildTarget": "front-end:build:development",
          "dev": true,
          "port": 4201
        },
        "production": {
          "buildTarget": "front-end:build:production",
          "dev": false
        }
      },
      "dependsOn": [
        {
          "target": "codegen:dev",
          "params": "forward"
        },
        {
          "target": "export-env",
          "params": "forward"
        }
      ]
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "front-end:build:production"
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "{projectRoot}/jest.config.ts",
        "passWithNoTests": true,
        "codeCoverage": true
      },
      "dependsOn": [
        {
          "target": "codegen:testing",
          "params": "forward"
        }
      ]
    },
    "get-secrets:dev": {
      "executor": "@pinecone-intern-monorepo/secrets:get",
      "defaultConfiguration": "development",
      "configurations": {
        "development": {
          "groups": ["2HF-INSTAGRAM-FRONTEND"],
          "env": "dev"
        }
      }
    },
    "get-secrets:testing": {
      "executor": "@pinecone-intern-monorepo/secrets:get",
      "defaultConfiguration": "testing",
      "configurations": {
        "testing": {
          "groups": ["2HF-INSTAGRAM-FRONTEND"],
          "env": "test"
        }
      }
    },
    "get-secrets:prod": {
      "executor": "@pinecone-intern-monorepo/secrets:get",
      "defaultConfiguration": "prod",
      "configurations": {
        "prod": {
          "groups": ["2HF-INSTAGRAM-FRONTEND"],
          "env": "prod"
        }
      }
    },
    "export-env": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["export $(cat apps/2FH/INSTAGRAM/front-end/.env | xargs)"],
        "parallel": false
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["rm -rf dist/apps/2FH/INSTAGRAM/front-end", "rm -rf .vercel"]
      }
    },
    "e2e": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["npx nx cypress example-frontend"],
        "parallel": false
      },
      "dependsOn": [{ "target": "cypress:clean" }]
    },
    "e2e:open": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["npx nx cypress front-end --watch"],
        "parallel": false
      },
      "dependsOn": [{ "target": "cypress:clean" }]
    },
    "cypress:clean": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "rm -rf apps/2FH/INSTAGRAM/front-end/cypress/results",
          "rm -rf apps/2FH/INSTAGRAM/front-end/.nyc_output",
          "rm -rf apps/2FH/INSTAGRAM/front-end/.cy",
          "rm -rf apps/2FH/INSTAGRAM/front-end/coverage"
        ],
        "parallel": false
      }
    },
    "cypress": {
      "executor": "@nx/cypress:cypress",
      "options": {
        "cypressConfig": "apps/2FH/INSTAGRAM/front-end/cypress.config.ts",
        "testingType": "e2e",
        "reporter": "mochawesome",
        "devServerTarget": "front-end:serve:development",
        "browser": "chrome"
      },
      "dependsOn": [
        {
          "target": "cypress-generate-all-page-json"
        },
        {
          "target": "get-secrets"
        },
        {
          "target": "export-env"
        }
      ]
    },
    "cypress-generate-all-page-json": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["npx ts-node apps/2FH/INSTAGRAM/front-end/cypress/utils/get-all-pages.ts"],
        "parallel": false
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx dotenv -v VERCEL_ORG_ID=team_KEsgJYvEcKmXZA6t8H4EyvFY -v VERCEL_PROJECT_ID=prj_U3y5BVrHoD2oOtoiQKcQt7nXUFfe -- vercel pull --yes --environment=preview --token=xjX9aLnOWs83krruPlLcXG30",
          "npx env-cmd -f apps/2FH/INSTAGRAM/front-end/.env vercel build --token=$VERCEL_TOKEN",
          "npx dotenv -v VERCEL_ORG_ID=team_KEsgJYvEcKmXZA6t8H4EyvFY -v VERCEL_PROJECT_ID=prj_U3y5BVrHoD2oOtoiQKcQt7nXUFfe -- vercel --archive=tgz --prebuilt --token=xjX9aLnOWs83krruPlLcXG30"
        ],
        "parallel": false
      },
      "dependsOn": [
        {
          "target": "clean"
        },
        {
          "target": "get-secrets:testing"
        },
        {
          "target": "export-env"
        },
        {
          "target": "codegen:testing",
          "params": "forward"
        }
      ]
    },
    "deploy-dev": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx dotenv -v VERCEL_ORG_ID=team_KEsgJYvEcKmXZA6t8H4EyvFY -v VERCEL_PROJECT_ID=prj_hUX7NCLPqPlsNPqDtJgGnnDwhPFD -- vercel pull --yes --environment=production --token=xjX9aLnOWs83krruPlLcXG30",
          "vercel build --prod --token=$VERCEL_TOKEN",
          "npx dotenv -v VERCEL_ORG_ID=team_KEsgJYvEcKmXZA6t8H4EyvFY -v VERCEL_PROJECT_ID=prj_hUX7NCLPqPlsNPqDtJgGnnDwhPFD -- vercel --prebuilt --prod --token=xjX9aLnOWs83krruPlLcXG30"
        ],
        "parallel": false
      },
      "dependsOn": [
        {
          "target": "clean"
        },
        {
          "target": "get-secrets:dev"
        },
        {
          "target": "export-env"
        },
        {
          "target": "codegen:dev",
          "params": "forward"
        }
      ]
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/2FH/INSTAGRAM/front-end/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "codegen:prod": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["graphql-codegen --config ./apps/2FH/INSTAGRAM/front-end/codegen.ts"]
      },
      "dependsOn": [
        {
          "target": "get-secrets:prod"
        }
      ]
    },
    "codegen:dev": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["graphql-codegen --config ./apps/2FH/INSTAGRAM/front-end/codegen.ts"]
      },
      "dependsOn": [{ "target": "get-secrets:dev" }]
    },
    "codegen:testing": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["graphql-codegen --config ./apps/2FH/INSTAGRAM/front-end/codegen.ts"]
      },
      "dependsOn": [
        {
          "target": "get-secrets:testing"
        }
      ]
    },
    "deploy-prod": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx dotenv -v VERCEL_ORG_ID=team_KEsgJYvEcKmXZA6t8H4EyvFY -v VERCEL_PROJECT_ID=prj_2HxhcxvSTOSAplI6s9nEqkuVOcMG -- vercel pull --yes --environment=production --token=xjX9aLnOWs83krruPlLcXG30",
          "vercel build --prod --token=$VERCEL_TOKEN",
          "npx dotenv -v VERCEL_ORG_ID=team_KEsgJYvEcKmXZA6t8H4EyvFY -v VERCEL_PROJECT_ID=prj_2HxhcxvSTOSAplI6s9nEqkuVOcMG -- vercel --prebuilt --prod --token=xjX9aLnOWs83krruPlLcXG30"
        ],
        "parallel": false
      },
      "dependsOn": [
        {
          "target": "clean"
        },
        {
          "target": "get-secrets:prod"
        },
        {
          "target": "export-env"
        },
        {
          "target": "codegen:prod",
          "params": "forward"
        }
      ]
    }
  },
  "tags": []
}
