{
  "name": "concert-2025-2cd",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/2CD/concert/backend",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/next:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "./dist/apps/2cd/concert/backend"
      },
      "configurations": {
        "development": {
          "outputPath": "./dist/apps/2cd/concert/backend"
        }
      },
      "dependsOn": [
        {
          "target": "codegen",
          "params": "forward"
        }
      ]
    },
    "serve": {
      "executor": "@nx/next:server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "concert-2025-2cd:build",
        "dev": true,
        "port": 8000
      },
      "configurations": {
        "development": {
          "buildTarget": "concert-2025-2cd:build:development",
          "dev": true
        },
        "production": {
          "buildTarget": "concert-2025-2cd:build:production",
          "dev": false
        }
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "apps/2CD/concert/backend/jest.config.ts",
        "passWithNoTests": true,
        "codeCoverage": true
      },
      "dependsOn": [
        {
          "target": "codegen",
          "params": "forward"
        }
      ]
    },
    "get-secrets:dev": {
      "executor": "@pinecone-intern-monorepo/secrets:get",
      "defaultConfiguration": "development",
      "configurations": {
        "development": {
          "groups": ["Concert-2025-2CD"],
          "env": "dev"
        }
      }
    },
    "get-secrets:testing": {
      "executor": "@pinecone-intern-monorepo/secrets:get",
      "defaultConfiguration": "testing",
      "configurations": {
        "testing": {
          "groups": ["Concert-2025-2CD"],
          "env": "test"
        }
      }
    },
    "get-secrets:prod": {
      "executor": "@pinecone-intern-monorepo/secrets:get",
      "defaultConfiguration": "prod",
      "configurations": {
        "prod": {
          "groups": ["Concert-2025-2CD"],
          "env": "prod"
        }
      }
    },
    "codegen": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["graphql-codegen --config ./apps/2CD/concert/backend/codegen.ts"]
      }
    },
    "export": {
      "executor": "@nx/next:export",
      "options": {
        "buildTarget": "Concert-2025-2CD:build:production"
      }
    },
    "export-env": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["export $(cat apps/2CD/concert/backend/.env | xargs)"],
        "parallel": false
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/2CD/concert/backend/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["rm -rf dist/apps/2CD/concert/backend", "rm -rf .vercel"]
      }
    },
    "preview": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx dotenv -v VERCEL_ORG_ID=team_dW0Tpe7DOfLR9xLRD2tSqkVp VERCEL_PROJECT_ID=prj_2BKm8SLpvc7fLtVhJSc9PuOM5QZn vercel pull --yes --environment=preview --token=wo6r3mEUqYSKN7pT5CGE4ucJ",
          "npx env-cmd -f apps/2CD/concert/backend/.env VERCEL_TOKEN=wo6r3mEUqYSKN7pT5CGE4ucJ vercel build --token=wo6r3mEUqYSKN7pT5CGE4ucJ",
          "npx dotenv -v VERCEL_ORG_ID=team_dW0Tpe7DOfLR9xLRD2tSqkVp VERCEL_PROJECT_ID=prj_2BKm8SLpvc7fLtVhJSc9PuOM5QZn VERCEL_TOKEN=wo6r3mEUqYSKN7pT5CGE4ucJ vercel --archive=tgz --prebuilt --token=wo6r3mEUqYSKN7pT5CGE4ucJ"
        ],
        "parallel": false
      },
      "dependsOn": [{ "target": "clean" }, { "target": "get-secrets:testing" }, { "target": "export-env" }, { "target": "codegen", "params": "forward" }]
    },
    "deploy-prod": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "VERCEL_ORG_ID=team_dW0Tpe7DOfLR9xLRD2tSqkVp VERCEL_PROJECT_ID=prj_5RoFKdsXCTzItPERQqCAu0XDLZGT VERCEL_TOKEN=wo6r3mEUqYSKN7pT5CGE4ucJ vercel pull --yes --environment=production --token=wo6r3mEUqYSKN7pT5CGE4ucJ",
          "VERCEL_TOKEN=wo6r3mEUqYSKN7pT5CGE4ucJ vercel build --prod --token=wo6r3mEUqYSKN7pT5CGE4ucJ",
          "VERCEL_ORG_ID=team_dW0Tpe7DOfLR9xLRD2tSqkVp VERCEL_PROJECT_ID=prj_5RoFKdsXCTzItPERQqCAu0XDLZGT VERCEL_TOKEN=wo6r3mEUqYSKN7pT5CGE4ucJ vercel --prebuilt --prod --token=wo6r3mEUqYSKN7pT5CGE4ucJ"
        ],
        "parallel": false
      },
      "dependsOn": [{ "target": "clean" }, { "target": "get-secrets:prod" }, { "target": "export-env" }, { "target": "codegen", "params": "forward" }]
    }
  },
  "tags": []
}
